{
	"name": "trivy-vulnerability-scanner",
	"displayName": "Trivy Vulnerability Scanner",
	"publisher": "AquaSecurityOfficial",
	"description": "A VS Code extension for Trivy.",
	"repository": "https://github.com/aquasecurity/trivy-vscode-extension",
	"icon": "images/aqualogo.png",
	"version": "0.2.0",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:trivy-vulnerability-scanner.scan"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "trivy-vulnerability-scanner.scan",
				"title": "Trivy Scan"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
